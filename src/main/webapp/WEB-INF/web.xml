<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app id="WebApp_ID">
	<display-name>room rent</display-name>
	<servlet>
		<servlet-name>registration</servlet-name>
		<display-name>registration</display-name>
		<description></description>
		<servlet-class>com.servlet.registration</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>login</servlet-name>
		<display-name>login</display-name>
		<description></description>
		<servlet-class>com.servlet.login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>book</servlet-name>
		<display-name>book</display-name>
		<description></description>
		<servlet-class>com.servlet.book</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>post</servlet-name>
		<display-name>post</display-name>
		<description></description>
		<servlet-class>com.servlet.post</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>registration</servlet-name>
		<url-pattern>/registration</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>book</servlet-name>
		<url-pattern>/book</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>post</servlet-name>
		<url-pattern>/post</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
	
	    <welcome-file>home.jsp</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>owner loging.jsp</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
